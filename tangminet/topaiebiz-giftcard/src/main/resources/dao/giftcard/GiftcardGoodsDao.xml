<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.topaiebiz.giftcard.dao.GiftcardGoodsDao">
    <select id="getGoodsByBatchId" parameterType="java.lang.Long" resultType="java.lang.Long">
    SELECT goodsId FROM t_giftcard_goods WHERE batchId=#{batchId} limit 30
    </select>
</mapper>